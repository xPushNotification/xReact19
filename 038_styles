помним что если мы называем файл some.module.css
то его можно импортировать как классы
и он будет генерировать айдишники
то есть ксс будет изолированным

import React from "react"
const AppInlineStyle = () => {
 return (
  <div>
   <h1 style={inlineStyles.title}>..</h1>
  </div>
 )
}

const inlineStyles = {
 title: {
  color:"blue",
  fontSize:"24px
 }
}

export default AppInlineStyle